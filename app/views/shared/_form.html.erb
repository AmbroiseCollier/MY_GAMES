<%= simple_form_for(@game) do |f| %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :picture_url %>
    <%= f.input :date, as: :date, html5: true %>
  <%# Add <% f.submit class: "create-game-button" %>
    <div class="buttons">
      <%= f.submit class:"create-game-button" %>
      <button class= "create-game-button" href="#">
        <%= link_to "Return to home page", root_path %>
      </button>
    </div>
  <% end %>
